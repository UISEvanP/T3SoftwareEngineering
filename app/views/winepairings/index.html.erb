<p id="notice"><%= notice %></p>

<h1>Winepairings</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Maindish</th>
      <th>wine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @winepairings.each do |winepairing| %>
      <tr>
        <td><%= winepairing.weight %></td>
        <td><%= winepairing.maindish %></td>
        <td><%= winepairing.wine %></td>
        <td><%= link_to 'Show', winepairing %></td>
        <td><%= link_to 'Edit', edit_winepairing_path(winepairing) %></td>
        <td><%= link_to 'Destroy', winepairing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Winepairing', new_winepairing_path %>
